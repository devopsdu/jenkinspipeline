pipeline {
   agent {
    label 'agent1'
    }
    tools{
     maven 'maven388'
     }
    stages {
        stage('checkout'){
            steps{
               git branch: 'master', url: 'https://github.com/thulasiv199/jenkinspipeline.git'
            }
        }
            stage('build'){
                steps{
                    sh "echo $PATH"
                    sh "mvn clean install" 
                }
            }
        
        
    }
    }
